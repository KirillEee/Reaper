-----------------Main----------------------------------------------------------
if game.PlaceId == 7056922815 then
    -----------------lib-----------------
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Peaper 2 by saul", "DarkTheme")


local Main = Window:NewTab("Peaper 2")
local MainSection = Main:NewSection("–êuto quest")
local RollSection = Main:NewSection("Rolls")
local pl = Window:NewTab("Local Player")
local plSection = pl:NewSection("Movement")
local Teles1 = Window:NewTab("Teleports")
local Teles = Teles1:NewSection("Teleport")
    --------------------ui----------------------------
    MainSection:NewKeybind("Toggle keybind", "Toggles ui", Enum.KeyCode.LeftAlt, function()
        Library:ToggleUI()
    end)

    -----------------------------------quest--------------------
    MainSection:NewKeybind("get quest for dibolus", "gets quest", Enum.KeyCode.F, function()
        local args = {
        [1] = "Scourge of the Realm",
        [2] = workspace.NPCs.DD_NightMare12
    }

    game:GetService("ReplicatedStorage").Remotes.TakeQuest:FireServer(unpack(args))

    end)
    -------exploration-------
    MainSection:NewButton("Get exploration quest read desc.", "Press this button and tp to diabolus", function()
    local args = {
        [1] = "Exploration!!!",
        [2] = workspace.NPCs.Just_Kenshii
    }

    game:GetService("ReplicatedStorage").Remotes.TakeQuest:FireServer(unpack(args))
    end)
    ----------------rolls------------------------


    --------secondary-----
    RollSection:NewKeybind("RollSecondary", "rolls secondary", Enum.KeyCode.F, function()
        local args = {
        [1] = "Roll Secondary"
    }

    game:GetService("ReplicatedStorage").Remotes.Purchase:FireServer(unpack(args))
    end)
    ----race-----
    RollSection:NewButton("Roll Race", "Rolls race", function()
        local args = {
            [1] = "Race Roll"
        }
    
        game:GetService("ReplicatedStorage").Remotes.Purchase:FireServer(unpack(args))
    end)
    ---Color-----
    RollSection:NewButton("Roll Color", "Rolls color", function()
        local args = {
            [1] = "Roll Color"
        }
    
        game:GetService("ReplicatedStorage").Remotes.Purchase:FireServer(unpack(args))
    end)    
    -----------tp------------------------
    MainSection:NewButton("Tp to diabolus", "Tps you to diabolus", function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-3847, -152, 882)
    end)-----Diabolus tp

    RollSection:NewButton("Tp to F33NY", "Teleports you to feeny", function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(127, 5, 1278)
    end)-----f33ny tp
    -----------------LocalPlayer-----------------

    plSection:NewToggle("Walkspeed", "Gives you 100 ws", function(on)
        if on then
                game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 100
        else
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
        end
    end)

    -------------------teleports-------------------
    Teles:NewButton("Tp to overworld", "Tp you to normal world", function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(18, 5, 1360)
    end)-------normal

    Teles:NewButton("Tp to Hueco", "Tps you to hueco", function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(5132, 198, 1226)
    end)---------hueco

    Teles:NewButton("Tp to hell", "Tps you to hell", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-3220, -133, 1041)
    end)------hell

    Teles:NewButton("Tp to soul society", "Tps you to soul society", function()
        placeid = 8934886191
        player = game.Players.LocalPlayer
        tp = game:GetService('TeleportService')
        
            tp:Teleport(placeid, player)
    end)
end

if game.PlaceId == 8934886191 then
