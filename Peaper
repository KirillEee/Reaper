local Teles = Teles1:NewSection("Teleport")
--------------UI------------------
local GUI = Window:NewTab("Settings")
local GUI1 = GUI:NewSection("Gui")
    ----------------------------------quest--------------------

    MainSection:NewKeybind("Get quest for Diabolus", "Gets quest", Enum.KeyCode.P, function()
        local args = {
        [1] = "Scourge of the Realm",
        [2] = workspace.NPCs.DD_NightMare12
    }

    game:GetService("ReplicatedStorage").Remotes.TakeQuest:FireServer(unpack(args))

    end)
    -------exploration-------
